{
  "version": 2,
  "env": {
    "DJANGO_SETTINGS_MODULE": "discovercode.settings"
  },
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}

